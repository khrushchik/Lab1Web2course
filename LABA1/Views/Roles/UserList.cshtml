@model IEnumerable<LABA1.Models.User>
@{
    ViewBag.Title = "Список пользователей";
}
<h2>Список пользователей</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>
<a class="btn btn-sm btn-primary" asp-controller="Users" asp-action="Index">Редактирование пользователей</a>
<img style="position: absolute; bottom:30px;right:15px; " src="~/css/fon.png" border="0" width=400px; />
