        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.GovNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.GovNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Body)
